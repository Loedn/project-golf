<%= simple_form_for [course, event], method: :patch, remote: true do |f| %>
  <div class="event-banner">
    <div class="event-details">
      <h1><input type="text" value="<%= event.title %>" name="event[title]" >
        <%= f.button :button, class: 'edit_event_title_button' do %>
          <i class="fa fa-pencil" aria-hidden="true"></i>
        <% end %>
      </h1>
      <p>Tue 10th Oct 10:00am</p>
    </div>
  </div>
<% end %>
